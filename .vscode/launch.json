{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Ejecutar",
            "sourceMaps": true,
            "stopOnEntry": true,
            "console": "internalConsole",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/src/index.ts",
            "outFiles": [
                "${workspaceRoot}/dist/*.js"
            ]
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Asociar a proceso",
            "port": 5858,
            "outFiles": []
        }
    ]
}